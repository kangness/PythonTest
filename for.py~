#!/usr/bin/env python
for country in ["Denmark","Finland","Norway","Sweden"]:
	print(country)

countries = ["Denmark","Finland","Norway","Sweden"]
for country in countries:
	print(country)

for letter in "ABCDEFGHIJKLMNOPQRSTUVWXYZ":
	if letter in "AEIOU":
		print(letter,"is a vowel")
	else:
		print(letter,"is a consonant")
